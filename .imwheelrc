# Speed up scrolling for chromium and pass unchanged for zoom
"^chromium-browser$"
    None, Up, Up, 4
    None, Down, Down, 4
    Shift_L,   Up,   Left, 2
    Shift_L,   Down,   Right, 2
    Control_L, Up,   Control_L|Up
    Control_L, Down, Control_L|Down


"^Thunar$"
    None, Up, Up, 4
    None, Down, Down, 4
    Shift_L,   Up,   Left, 2
    Shift_L,   Down,   Right, 2
    Control_L, Up,   Control_L|Up
    Control_L, Down, Control_L|Down
